{
  "name": "tabby-client",
  "version": "0.0.1",
  "description": "Tabby Client is a Node libirary that provides code completions from the Tabby Server.",
  "lib": "index.js",
  "repository": "https://github.com/TabbyML/tabby",
  "scripts": {
    "clean": "rm -rf ./generated && rm ./index.*",
    "openapi-codegen": "openapi --input ../../docs/openapi.json --output ./generated --client axios --name Tabby --indent 2",
    "build": "yarn openapi-codegen && webpack"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.24.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "typescript-declaration-webpack-plugin": "^0.2.2",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "form-data": "^4.0.0"
  }
}
